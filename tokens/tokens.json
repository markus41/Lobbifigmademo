{
  "$schema": "https://json.schemastore.org/design-tokens",
  "meta": {
    "name": "lobbi-design-tokens",
    "version": "1.0.0",
    "source": [
      "src/theme/theme.base.json",
      "src/styles/theme.css",
      "src/styles/org-themes.css",
      "src/theme/orgs/*.json"
    ]
  },
  "color": {
    "surface": {
      "base": "#FDF7ED",
      "raised": "#FFF9F0",
      "overlay": "rgba(255, 250, 242, 0.86)",
      "panel": "rgba(255, 255, 255, 0.92)",
      "canvas": "#FAF6E9",
      "card": "#FFFFFF",
      "cardBorder": "#EDE8DD"
    },
    "text": {
      "primary": "#151515",
      "secondary": "#4B4B4B",
      "tertiary": "#7A7A7A",
      "muted": "#9A9A9A"
    },
    "brand": {
      "primary": "#D4AF37",
      "primaryLight": "#F4D03F",
      "primaryPale": "#F5E6A3",
      "primaryDark": "#8B7330",
      "glow": "rgba(212,175,55,0.22)"
    },
    "neutral": {
      "50": "#FAFAF9",
      "100": "#F5F5F4",
      "200": "#E7E5E4",
      "400": "#A8A29E",
      "600": "#57534E",
      "800": "#292524",
      "900": "#1C1917"
    },
    "semantic": {
      "success": "#059669",
      "warning": "#D97706",
      "error": "#DC2626",
      "info": "#0284C7"
    },
    "line": {
      "subtle": "rgba(21,21,21,0.08)",
      "gold": "rgba(212,175,55,0.35)",
      "goldSubtle": "rgba(212,175,55,0.18)",
      "hairline": "rgba(0,0,0,0.05)",
      "hairlineGold": "rgba(212,175,55,0.20)"
    }
  },
  "spacing": {
    "0": "0",
    "1": "4px",
    "2": "8px",
    "3": "12px",
    "4": "16px",
    "5": "20px",
    "6": "24px",
    "7": "28px",
    "8": "32px",
    "9": "36px",
    "10": "40px",
    "12": "48px",
    "14": "56px",
    "16": "64px",
    "20": "80px",
    "24": "96px",
    "px": "1px",
    "0.5": "2px",
    "1.5": "6px",
    "2.5": "10px",
    "3.5": "14px",
    "container": "max(1.5rem, 4vw)",
    "sectionY": "8rem",
    "cardPadding": "20px",
    "inputPadding": "16px"
  },
  "typography": {
    "fontFamily": {
      "display": "'DM Serif Display', 'Playfair Display', Georgia, serif",
      "body": "'Sora', 'DM Sans', system-ui, sans-serif",
      "mono": "'IBM Plex Mono', 'JetBrains Mono', monospace",
      "accent": "'Cormorant Garamond', Georgia, serif"
    },
    "size": {
      "xs": "0.75rem",
      "sm": "0.875rem",
      "base": "1rem",
      "lg": "1.125rem",
      "xl": "1.25rem",
      "2xl": "1.5rem",
      "3xl": "1.875rem",
      "4xl": "2.25rem",
      "5xl": "3rem",
      "6xl": "3.75rem",
      "7xl": "4.5rem"
    },
    "weight": {
      "light": 300,
      "regular": 400,
      "medium": 500,
      "semibold": 600,
      "bold": 700
    },
    "lineHeight": {
      "none": 1,
      "tight": 1.2,
      "snug": 1.375,
      "normal": 1.5,
      "relaxed": 1.625,
      "loose": 1.8
    },
    "tracking": {
      "brand": "0.35em",
      "caps": "0.20em",
      "tight": "-0.02em",
      "wide": "0.05em",
      "widest": "0.15em"
    }
  },
  "radius": {
    "none": "0",
    "xs": "4px",
    "sm": "8px",
    "md": "12px",
    "lg": "16px",
    "xl": "20px",
    "2xl": "24px",
    "3xl": "32px",
    "full": "9999px",
    "panel": "24px",
    "input": "12px",
    "button": "10px",
    "card": "16px"
  },
  "shadow": {
    "none": "none",
    "xs": "0 1px 2px rgba(0,0,0,0.03)",
    "sm": "0 2px 4px rgba(0,0,0,0.04)",
    "md": "0 4px 12px rgba(0,0,0,0.06)",
    "lg": "0 8px 24px rgba(0,0,0,0.08)",
    "xl": "0 16px 40px rgba(0,0,0,0.10)",
    "2xl": "0 24px 60px rgba(0,0,0,0.12)",
    "panel": "0 8px 32px rgba(0,0,0,0.06), 0 1px 2px rgba(0,0,0,0.02)",
    "float": "0 12px 40px rgba(0,0,0,0.08)",
    "goldSoft": "0 12px 40px rgba(212,175,55,0.14)",
    "goldGlow": "0 0 40px -10px rgba(212,175,55,0.3)",
    "inner": "inset 0 2px 4px rgba(0,0,0,0.06)",
    "focus": "0 0 0 3px rgba(212,175,55,0.25)"
  },
  "border": {
    "none": "none",
    "hairline": "0.5px solid rgba(0,0,0,0.05)",
    "hairlineGold": "0.5px solid rgba(212,175,55,0.20)",
    "thin": "1px solid rgba(0,0,0,0.08)",
    "default": "1px solid #EDE8DD",
    "thick": "2px solid #EDE8DD",
    "gold": "1px solid rgba(212,175,55,0.35)",
    "goldThick": "2px solid #D4AF37",
    "decorative": "3px double #D4AF37"
  },
  "motion": {
    "durationMs": {
      "instant": 0,
      "fast": 150,
      "normal": 300,
      "slow": 500,
      "heroIntro": 2000,
      "gateToAuth": 900,
      "focus": 350,
      "hover": 450,
      "pageTransition": 800,
      "ambientCycle": 12000
    },
    "easingCSS": {
      "luxInOut": "cubic-bezier(0.22, 1, 0.36, 1)",
      "luxOut": "cubic-bezier(0.16, 1, 0.3, 1)",
      "fade": "cubic-bezier(0.4, 0, 0.2, 1)",
      "bounce": "cubic-bezier(0.68, -0.55, 0.265, 1.55)"
    }
  },
  "zIndex": {
    "base": 0,
    "dropdown": 10,
    "sticky": 20,
    "fixed": 30,
    "overlay": 40,
    "modal": 50,
    "popover": 60,
    "tooltip": 70,
    "toast": 80
  },
  "breakpoints": {
    "sm": "640px",
    "md": "768px",
    "lg": "1024px",
    "xl": "1280px",
    "2xl": "1536px"
  },
